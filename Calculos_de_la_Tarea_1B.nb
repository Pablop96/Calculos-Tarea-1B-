Cell[BoxData[
 RowBox[{"ConditionalExpression", "[", 
  RowBox[{
   FractionBox[
    RowBox[{
     SuperscriptBox["2", 
      RowBox[{
       RowBox[{"-", 
        FractionBox["1", "2"]}], "-", 
       FractionBox["\[Nu]", "2"]}]], " ", 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{
        FractionBox["1", "2"], "+", 
        FractionBox[
         SuperscriptBox["t", "2"], 
         RowBox[{"2", " ", "\[Nu]"}]]}], ")"}], 
      RowBox[{
       RowBox[{"-", 
        FractionBox["1", "2"]}], "-", 
       FractionBox["\[Nu]", "2"]}]], " ", 
     RowBox[{"Gamma", "[", 
      RowBox[{
       FractionBox["1", "2"], "+", 
       FractionBox["\[Nu]", "2"]}], "]"}]}], 
    RowBox[{
     SqrtBox["\[Pi]"], " ", 
     SqrtBox["\[Nu]"], " ", 
     RowBox[{"Gamma", "[", 
      FractionBox["\[Nu]", "2"], "]"}]}]], ",", 
   RowBox[{
    RowBox[{
     RowBox[{"Re", "[", 
      FractionBox[
       SuperscriptBox["t", "2"], "\[Nu]"], "]"}], ">", 
     RowBox[{"-", "1"}]}], "&&", 
    RowBox[{
     RowBox[{"Re", "[", "\[Nu]", "]"}], ">", 
     RowBox[{"-", "1"}]}]}]}], "]"}]], "Output",
 CellChangeTimes->{3.816566631182225*^9},
 CellLabel->"Out[37]=",ExpressionUUID->"82b2a23c-0838-4b51-a6a6-233e82a7e48a"]
